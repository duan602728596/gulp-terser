{
  "name": "gulp-terser",
  "version": "2.1.0",
  "description": "gulp plugin, compressed es6+ code.",
  "scripts": {
    "build": "npm run clean && NODE_OPTIONS=\"--experimental-json-modules\" gulp",
    "clean": "rimraf ./lib && rimraf ./esm",
    "test": "mocha \"./test/build/**/*.js\"",
    "test:gulp": "gulp --gulpfile ./test/gulpfile.mjs",
    "eslint": "eslint ./ --fix"
  },
  "main": "dist/cjs.js",
  "license": "MIT",
  "keywords": [
    "gulp",
    "gulpplugin",
    "uglify-es",
    "terser"
  ],
  "author": {
    "name": "段昊辰",
    "email": "duanhaochen@126.com",
    "url": "https://github.com/duan602728596"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/duan602728596/gulp-terser.git"
  },
  "dependencies": {
    "plugin-error": "^1.0.1",
    "terser": "^5.10.0",
    "through2": "^4.0.2",
    "vinyl-sourcemaps-apply": "^0.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.4",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-syntax-import-assertions": "^7.16.7",
    "@types/node": "^17",
    "@types/through2": "^2",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "chai": "^4.3.6",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.9.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "mocha": "^9.2.0",
    "rimraf": "^3.0.2",
    "typescript": "next"
  },
  "engines": {
    "node": ">=10"
  },
  "lint-staged": {
    "**/*.{js,jsx,mjs,cjs,ts,tsx,mts,cts}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
